{
  "name": "puzzik",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "i18n": "npm run i18n-extract && npm run i18n-createJson",
    "i18n-createJson": "tsc tools/dev-localizer.ts && node tools/dev-localizer.js",
    "i18n-extract": "node_modules\\.bin\\ngc -p tsconfig.app.json && node_modules\\.bin\\localize-extract -l de-DE -s ./dist/out-locl/**/*.js -f xlf -o src/locale/messages.de-DE.xlf"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.7",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "xliff": "^5.7.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.6",
    "@angular/cli": "~13.2.6",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/localize": "^13.2.7",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
